{
  "extends": "airbnb-base",
  "env": {
    "es6": true,
    "node": true,
    "jest": true
  },
  "parserOptions": {
    "ecmaVersion": 2017,
    "sourceType": "script"
  },
  "rules": {
    "no-use-before-define": ["error", {"functions": false}],
    "comma-dangle": ["warn", "never"],
    "no-param-reassign": "warn",
    "indent": ["warn", 4],
    "no-console": "off",
    "no-plusplus": "off",
    "no-await-in-loop": "off",
    "class-methods-use-this": "off",
    "no-underscore-dangle": "off",
    "object-curly-newline": "off"
  }
}
