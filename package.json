  },
  "now": {
    "name": "k1logram",
    "alias": "k1logram.now.sh",
    "env": {
      "NODE_ENV": "production",
      "SECRET_KEY": "@secret_key",
      "GITHUB_CLIENT_ID": "@gh_client_id",
      "GITHUB_CLIENT_SECRET": "@gh_client_secret",
      "HRUDB_TOKEN": "@hrudb_token"
    }
